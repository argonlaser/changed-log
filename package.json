{
  "name": "changed-log",
  "version": "0.7.0",
  "description": "Returns all commit messages between 2 versions of an NPM module",
  "main": "src/changed-log.js",
  "bin": {
    "changed-log": "bin/changed-log.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha -R spec src/**/*-spec.js && npm run chalk",
    "watch": "mocha -R spec --watch src/**/*-spec.js",
    "example": "node bin/changed-log.js next-update 0.8.0 0.8.3",
    "chalk": "node bin/changed-log.js chalk 0.3.0 0.5.1",
    "lint": "eslint bin/*.js src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/changed-log.git"
  },
  "keywords": [
    "change",
    "changelog",
    "changes",
    "version",
    "release",
    "module",
    "package",
    "npm"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/changed-log/issues"
  },
  "homepage": "https://github.com/bahmutov/changed-log",
  "dependencies": {
    "bluebird": "2.10.2",
    "chalk": "1.1.1",
    "check-more-types": "2.1.1",
    "debug": "2.2.0",
    "github": "0.2.4",
    "inquirer": "0.10.1",
    "lazy-ass": "0.6.0",
    "lodash": "3.10.1",
    "package-json": "2.0.1",
    "ramda": "0.15.1",
    "update-notifier": "0.5.0"
  },
  "devDependencies": {
    "eslint": "0.24.0",
    "mocha": "2.3.3",
    "pre-git": "0.6.2"
  },
  "pre-commit": "npm test",
  "post-commit": "npm version"
}
